{
  "name": "pyarn",
  "version": "0.8.0",
  "main": "index.js",
  "author": "James Kyle <me@thejameskyle.com>",
  "license": "MIT",
  "repository": "https://github.com/pyarn/pyarn",
  "bin": {
    "pyarn": "./bin.js"
  },
  "files": [
    "dist",
    "index.js",
    "bin.js"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "flow": "flow status",
    "test": "jest --coverage",
    "build:legacy": "BABEL_ENV=legacy babel src -d dist/legacy",
    "build:modern": "BABEL_ENV=modern babel src -d dist/modern",
    "build": "yarn run clean && yarn build:legacy && yarn build:modern",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.51.0",
    "jest": "^20.0.4",
    "jest-fixtures": "^0.2.0"
  },
  "dependencies": {
    "babel-runtime": "^6.25.0",
    "chalk": "^2.0.1",
    "clean-stack": "^1.3.0",
    "cmd-shim": "^2.0.2",
    "cross-spawn": "^5.1.0",
    "detect-indent": "^5.0.0",
    "find-up": "^2.1.0",
    "globby": "^6.1.0",
    "make-dir": "^1.0.0",
    "meow": "^3.7.0",
    "multimatch": "^2.1.0",
    "p-limit": "^1.1.0",
    "parse-json": "^2.2.0",
    "path-is-inside": "^1.0.2",
    "pkg-up": "^2.0.0",
    "read-cmd-shim": "^1.0.1",
    "read-pkg": "^2.0.0",
    "rimraf": "^2.6.1",
    "semver": "^5.4.1",
    "sort-object": "^3.0.2",
    "typeable-promisify": "^2.0.1"
  },
  "jest": {
    "resetMocks": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/dist/",
      "/flow-typed/"
    ],
    "haste": {}
  }
}
